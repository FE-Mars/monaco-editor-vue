{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential"
  ],
  "rules": {
    "no-console": 0,
    "no-debugger": 0,
    "no-useless-escape": 0,
    "global-require": 0
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}